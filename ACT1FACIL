monedas = [10, 25, 5, 50, 1, 20]

suma_total = sum(monedas)

promedio = suma_total / len(monedas)

moneda_mas_valiosa = max(monedas)

print("Lista de monedas de oro:", monedas)
print("Suma total de monedas:", suma_total)
print("Promedio de valor de las monedas:", promedio)
print("Moneda m√°s valiosa:", moneda_mas_valiosa)
